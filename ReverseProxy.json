{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "AllowedHosts": "*",
    "ReverseProxy": {
      "Routes": {
        "logmonitor": {
          "ClusterId": "logmonitor",
          "Match": {
            "Path": "/logmonitor/{**catch-all}"
          }
        }
      },
      "Clusters": {
        "logmonitor": {
          "Destinations": {
            "logmonitor/destination1": {
              "Address": "http://localhost:5173/"
            }
          }
        }
      }
    }
  }